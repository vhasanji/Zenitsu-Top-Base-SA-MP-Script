CMD:chopcar(playerid, params[]) {
	new vehid = GetPlayerVehicleID(playerid);
	new clientMessage[128];
	if(ChopInfo[cTime] > 0)
	{
	    return SM(playerid, COLOR_SYNTAX, "Chop chop cooldown %i hours. You can't chop it now.", ChopInfo[cTime]);
	}
	if((gettime() - PlayerInfo[playerid][pLastChop]) < 7200000)
	{
		return SCM(playerid, COLOR_LIGHTBLUE , "[ERROR]:"WHITE" You need to wait "WHITE"50 minues"REDORANGE" before sending another message.");
	}
	if(IsLawEnforcement(playerid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "You can't chop the vehicle as a law enforcer. Ask your boss for a raise.");
	}
	new count;
	foreach(new i : Player)
	{
	    if(IsLawEnforcement(i) && PlayerInfo[i][pDuty] == 1)
	    {
	        count++;
		}
	}

	if(count < 4)
	{
	    return SCM(playerid, COLOR_SYNTAX, "There needs to be at least 4+ SHERIFF's online in order to chop this vehicle.");
	}

	if(!VehicleInfo[vehid][vOwnerID])
	{
	    return SCM(playerid, COLOR_SYNTAX, "This vehicle isn't owned by any particular person.");
	}
	// If player isn't in a car (driver or passenger), abort.
	if(!IsPlayerInAnyVehicle(playerid)) {
		SCM(playerid, COLOR_GREY, " You need to be in a car to sell it.");
		return 1;
	}
	// If player isn't at the drop point, abort.
	if(!IsPlayerInRangeOfPoint(playerid, 5.0, -1851.7740,-1699.5142,40.8672)) {
		SCM(playerid, COLOR_GREY, " You need to be at the chop shop to chop a stolen car.");
		return 1;
	}

	/*// If player isn't high enough level, abort.
	if(PlayerInfo[playerid][pLevel] < 2) {
		SCM(playerid, COLOR_GREY, "You have to be level 2 or higher to be able to sell stolen vehicles.");
		return 1;
	}*/

	GameTextForPlayer(playerid, "~r~Chopping Vehicle~n~~w~it will take 1 minute...", 60000, 1);
	TogglePlayerControllable(playerid, 0);
	SetTimerEx("ChoppingVehicle", 60000, false, "i", playerid);

	// Send RR a GPS coord to where the car was dumped.
	format(clientMessage, sizeof(clientMessage), "____________ DISPATCH PANEL _____________");
	SendFactionTypeMessage(FACTION_POLICE, COLOR_YELLOW, clientMessage);
	format(clientMessage,sizeof(clientMessage),"A "REDORANGE"%s"WHITE" has been reported "REDORANGE"vehicle stolen"WHITE". Location sent to your GPS...", GetVehicleName(vehid));
	SendFactionTypeMessage(FACTION_POLICE, COLOR_WHITE, clientMessage);

	PlayerInfo[playerid][pLastChop] = gettime();

	ResetChop();

	foreach(new p : Player)
	{
		if(GetFactionType(p) == FACTION_POLICE)
		{
			SetPlayerCheckpoint(p, -1851.7740,-1699.5142,40.8672, 4.0);
		}
	}
	return 1;
}

