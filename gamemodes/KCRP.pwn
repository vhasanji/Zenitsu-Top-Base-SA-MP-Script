	if(IsVehicleOwner(playerid, vehicleid) && PlayerInfo[playerid][pVehicleKeys] != vehicleid)
	{
		SendProximityMessage(playerid, 20.0, COLOR_GREY, "{C2A2DA} %s parks their %s.", GetRPName(playerid), GetVehicleName(vehicleid));
	 	SM(playerid, COLOR_AQUA, "You have parked your "SVRCLR"%s{CCFFFF} which will spawn in this spot from now on.", GetVehicleName(vehicleid));

		// Save the vehicle's information.
		GetVehiclePos(vehicleid, VehicleInfo[vehicleid][vPosX], VehicleInfo[vehicleid][vPosY], VehicleInfo[vehicleid][vPosZ]);
		GetVehicleZAngle(vehicleid, VehicleInfo[vehicleid][vPosA]);

	    VehicleInfo[vehicleid][vInterior] = GetPlayerInterior(playerid);
	    VehicleInfo[vehicleid][vWorld] = GetPlayerVirtualWorld(playerid);

		// Update the database record with the new information, then despawn the vehicle.
		mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE vehicles SET pos_x = '%f', pos_y = '%f', pos_z = '%f', pos_a = '%f', interior = %i, world = %i WHERE id = %i", VehicleInfo[vehicleid][vPosX], VehicleInfo[vehicleid][vPosY], VehicleInfo[vehicleid][vPosZ], VehicleInfo[vehicleid][vPosA], VehicleInfo[vehicleid][vInterior], VehicleInfo[vehicleid][vWorld], VehicleInfo[vehicleid][vID]);
		mysql_tquery(connectionID, queryBuffer);
		DespawnVehicle(vehicleid);

		// Finally, we reload the vehicle from the database.
		mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "SELECT * FROM vehicles WHERE id = %i", id);
		mysql_tquery(connectionID, queryBuffer, "OnPlayerSpawnVehicle", "ii", playerid, true);
	}
	else if(iVehicle != 0 && iBusiness != INVALID_BUSINESS_ID)
	{
	 	if(!IsBusinessOwner(playerid, iBusiness))
	 	{
	 		return SendClientMessage(playerid, COLOR_WHITE, "You're not authorized to park this vehicle.");
	 	}
		new iSlot = GetBusinessCarSlot(vehicleid), Float: XYZ[4];

		GetVehiclePos(vehicleid, XYZ[0], XYZ[1], XYZ[2]);
		GetVehicleZAngle(vehicleid, XYZ[3]);

		BusinessInfo[iBusiness][bParkPosX][iSlot] = XYZ[0];
		BusinessInfo[iBusiness][bParkPosY][iSlot] = XYZ[1];
		BusinessInfo[iBusiness][bParkPosZ][iSlot] = XYZ[2];
		BusinessInfo[iBusiness][bParkAngle][iSlot] = XYZ[3];

		DestroyVehicle(BusinessInfo[iBusiness][bVehID][iSlot]);
		BusinessInfo[iBusiness][bVehID][iSlot] = CreateVehicle(BusinessInfo[iBusiness][bModel][iSlot], BusinessInfo[iBusiness][bParkPosX][iSlot], BusinessInfo[iBusiness][bParkPosY][iSlot], BusinessInfo[iBusiness][bParkPosZ][iSlot],
		BusinessInfo[iBusiness][bParkAngle][iSlot], 0, 0, -1);

        SaveDealershipVehicle(iBusiness, iSlot);
		SendClientMessage(playerid, COLOR_WHITE, "You've parked this vehicle.");
		return 1;
	}
	else if(!IsVehicleOwner(playerid, vehicleid) && PlayerInfo[playerid][pVehicleKeys] != vehicleid)
	{
		return SendClientMessage(playerid, COLOR_SYNTAX, "You can't park this vehicle as it doesn't belong to you.");
	}

	return 1;
}

// Dealership
CMD:dealershiprespawn(playerid, params[])
{
	new businessid = GetInsideBusiness(playerid);
	if(businessid == -1 || !IsBusinessOwner(playerid, businessid))
	{
	    return SendClientMessage(playerid, COLOR_SYNTAX, "You're not near any business that you own.");
	}
	if(businessid == INVALID_BUSINESS_ID) return SendClientMessage(playerid, COLOR_GREY, "You are not in a business, use /switchbiz first.");
	SendClientMessage(playerid, COLOR_GREY, "Respawning dealership vehicles..");
	for (new i; i < MAX_BUSINESS_DEALERSHIP_VEHICLES; i++)
	{
		if(BusinessInfo[businessid][bVehID][i] != 0) SetVehicleToRespawn(BusinessInfo[businessid][bVehID][i]);
	}
	return 1;
}

CMD:editcarprice(playerid, params[])
{
	new businessid = GetNearbyCarBusinessEx(playerid);
    //new businessid = GetInsideBusiness(playerid);
	if(businessid == -1 || !IsBusinessOwner(playerid, businessid))
	{
	    return SendClientMessage(playerid, COLOR_SYNTAX, "You're not near any business that you own.");
	}
	else if(BusinessInfo[businessid][bType] != BUSINESS_DEALERSHIP)
	{
		SendClientMessage(playerid, COLOR_GREY, "You don't own a vehicle dealership.");
	}
	else if(!IsPlayerInRangeOfPoint(playerid, 20.0, BusinessInfo[businessid][bPosX], BusinessInfo[businessid][bPosY], BusinessInfo[businessid][bPosZ])) {
		SendClientMessage(playerid, COLOR_GREY, "You need to be standing near the dealership entrance.");
	}
	else
	{
		new vehicleid, amount;
		if(sscanf(params, "dd", vehicleid, amount)) {
			SendClientMessage(playerid, COLOR_GREY, "USAGE: /editcarprice [VehicleID] [Price]");
		}
		else if(businessid != GetCarBusiness(vehicleid)) {
			SendClientMessage(playerid, COLOR_GREY, "That vehicle is not part of your dealership.");
		}
		else if(amount < 0) {
			SendClientMessage(playerid, COLOR_GREY, "The price can't be set below 0");
		}
		else
  		{
 			new
			iSlot = GetBusinessCarSlot(vehicleid),
			Message[128];

			BusinessInfo[businessid][bVPrice][iSlot] = amount;
			format(Message, sizeof(Message), "%s For Sale | Price: $%s", GetVehicleName(BusinessInfo[businessid][bVehID][iSlot]), number_format(BusinessInfo[businessid][bVPrice][iSlot]));
            UpdateDynamic3DTextLabelText(BusinessInfo[businessid][bVehicleLabel][iSlot], COLOR_LIGHTBLUE, Message);
			format(Message, sizeof(Message), "%s price has been set to $%s", GetVehicleName(vehicleid), number_format(amount));
			SendClientMessage(playerid, COLOR_WHITE, Message);
			SaveDealershipVehicle(businessid, iSlot);
		}
	}
	return 1;
}

CMD:deletecdveh(playerid, params[]) {
	if(PlayerInfo[playerid][pAdmin] >= 4) {

		new
		    iBusiness,
			iVehicle;

		if(sscanf(params, "ii", iBusiness, iVehicle )) {
			SendClientMessage(playerid, COLOR_GREY, "USAGE: /deletecdveh [business ID] [model id]");
		}
		else if(BusinessInfo[iBusiness][bType] != BUSINESS_DEALERSHIP) {
			return SendClientMessage(playerid, COLOR_GREY, "Business is not a car dealership!");
		}
		else if(iBusiness != GetCarBusiness(iVehicle)) {
			SendClientMessage(playerid, COLOR_GREY, "That vehicle isn't a dealership vehicle.");
		}
		else {

			new
				ID = GetBusinessCarSlot(iVehicle);

			if(BusinessInfo[iBusiness][bVehID][ID] != INVALID_VEHICLE_ID) {
			    if(IsValidDynamic3DTextLabel(BusinessInfo[iBusiness][bVehicleLabel][ID])) DestroyDynamic3DTextLabel(BusinessInfo[iBusiness][bVehicleLabel][ID]), BusinessInfo[iBusiness][bVehicleLabel][ID] = Text3D:-1;
                DestroyVehicle(BusinessInfo[iBusiness][bVehID][ID]);
               	BusinessInfo[iBusiness][bModel][ID] = 0;
				BusinessInfo[iBusiness][bParkPosX][ID] = 0;
  				BusinessInfo[iBusiness][bParkPosY][ID] = 0;
			   	BusinessInfo[iBusiness][bParkPosZ][ID] = 0;
		   	 	BusinessInfo[iBusiness][bParkAngle][ID] = 0;
		   	 	BusinessInfo[iBusiness][bVehID][ID] = 0;
		   	 	BusinessInfo[iBusiness][bVPrice][ID] = 0;
		   	 	SaveDealershipVehicle(iBusiness, ID);
	   	 		return 1;
       		}
			return SendClientMessage(playerid, COLOR_GREY, "The max number of vehicles for this business has been reached.");
		}
	}
	else SendClientMessage(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" You are not authorized to use this command.");
	return 1;
}


CMD:createcdveh(playerid, params[])
{
	new iBusiness, iVehicle, iColors[2], Float: fVehPos[4], label[50], model[20];
	if(sscanf(params, "is[20]ii", iBusiness, model, iColors[0], iColors[1]))
	{
		return SendClientMessage(playerid, COLOR_GREY, "USAGE: /createcdveh [business ID] [model id] [color 1] [color 2]");
	}
	if(PlayerInfo[playerid][pAdmin] < 4)
	{
		return SendClientMessage(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" You are not authorized to use this command.");
	}
	if((iVehicle = GetVehicleModelByName(model)) == 0)
	{
	    return SendClientMessage(playerid, COLOR_SYNTAX, "Invalid vehicle model.");
	}
	if(!(-1 <= iColors[0] <= 255) || !(-1 <= iColors[1] <= 255))
	{
	    return SendClientMessage(playerid, COLOR_SYNTAX, "Invalid color. Valid colors range from -1 to 255.");
	}
	if(BusinessInfo[iBusiness][bType] != BUSINESS_DEALERSHIP)
	{
		return SendClientMessage(playerid, COLOR_GREY, "Business is not a car dealership!");
	}
	for(new i; i < MAX_BUSINESS_DEALERSHIP_VEHICLES; i++)
	{
		if(BusinessInfo[iBusiness][bVehID][i] == 0)
		{
			GetPlayerPos(playerid, fVehPos[0], fVehPos[1], fVehPos[2]);
			GetPlayerFacingAngle(playerid, fVehPos[3]);
			BusinessInfo[iBusiness][bVehID][i] = CreateVehicle(iVehicle, fVehPos[0], fVehPos[1], fVehPos[2], fVehPos[3], iColors[0], iColors[1], -1);

			BusinessInfo[iBusiness][bModel][i] = iVehicle;

			BusinessInfo[iBusiness][bParkPosX][i] = fVehPos[0];
			BusinessInfo[iBusiness][bParkPosY][i] = fVehPos[1];
			BusinessInfo[iBusiness][bParkPosZ][i] = fVehPos[2];
			BusinessInfo[iBusiness][bParkAngle][i] = fVehPos[3];

			format(label, sizeof(label), "%s For Sale | Price: $%s", GetVehicleName(BusinessInfo[iBusiness][bVehID][i]), number_format(BusinessInfo[iBusiness][bVPrice][i]));
			BusinessInfo[iBusiness][bVehicleLabel][i] = CreateDynamic3DTextLabel(label,COLOR_LIGHTBLUE,BusinessInfo[iBusiness][bParkPosX][i], BusinessInfo[iBusiness][bParkPosY][i], BusinessInfo[iBusiness][bParkPosZ][i],8.0,INVALID_PLAYER_ID, BusinessInfo[iBusiness][bVehID][i]);

			BusinessInfo[iBusiness][DealershipVehStock][i] = 1;
			SaveDealershipVehicle(iBusiness, i);
			return 1;
		}
	}
	return 1;
}

