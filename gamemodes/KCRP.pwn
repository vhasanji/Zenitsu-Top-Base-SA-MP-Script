CMD:give(playerid, params[])
{
	new targetid, option[14], param[32], amount;
	if(sscanf(params, "us[14]S()[32]", targetid, option, param))
	{
	    SCM(playerid, COLOR_SYNTAX, "[USAGE]: /give [playerid] [option]");
	    SCM(playerid, COLOR_WHITE, "Available options: Weapon, Materials, Pot, Crack, Meth, Painkillers, Cigars, Spraycans");
	    SCM(playerid, COLOR_WHITE, "Available options: GasCan, Seeds, Ephedrine, DirtyCash, Diamonds");
	    SCM(playerid, COLOR_WHITE, "Available options: Medkit, Repairkit, Food, Drink, Parts, Vest, Lockpick");
	    return 1;
	}
	if(!IsPlayerConnected(targetid) || !IsPlayerInRangeOfPlayer(playerid, targetid, 5.0))
	{
	    return SCM(playerid, COLOR_SYNTAX, "The player specified is disconnected or out of range.");
	}
	if(targetid == playerid)
	{
	    return SCM(playerid, COLOR_SYNTAX, "You can't use this command on yourself.");
	}
    if(PlayerInfo[playerid][pTazedTime] > 0 || PlayerInfo[playerid][pInjured] > 0 || PlayerInfo[playerid][pHospital] > 0 || PlayerInfo[playerid][pTied] > 0 || PlayerInfo[playerid][pTied] > 0 || PlayerInfo[playerid][pCuffed] > 0 || PlayerInfo[playerid][pJailTime] > 0 || PlayerInfo[playerid][pJoinedEvent] > 0 || PlayerInfo[playerid][pPaintball])
	{
	    return SCM(playerid, COLOR_SYNTAX, "You can't use this command at the moment.");
	}
	if(!strcmp(option, "weapon", true))
	{
	    new weaponid = GetScriptWeapon(playerid);

        if(weaponid == 22 || weaponid == 26 || weaponid == 28 || weaponid == 32)
		{
	    return SCM(playerid, COLOR_SYNTAX, "You are not allowed to give this weapon to anyone.");
		}
	    if(!weaponid)
	    {
	        return SCM(playerid, COLOR_ERROR, "[ERROR]:"WHITE" To give a weapon, you must be holding it or this weapon are not allowed to give to anyone.");
	    }
	    if(PlayerInfo[targetid][pWeapons][weaponSlotIDs[weaponid]] > 0)
	    {
	        return SCM(playerid, COLOR_ERROR, "[ERROR]:"WHITE" That player already has a weapon in that slot.");
	    }
	    if(PlayerInfo[targetid][pHours] < 2 || PlayerInfo[targetid][pWeaponRestricted] > 0)
	    {
	        return SCM(playerid, COLOR_ERROR, "[ERROR]:"WHITE" That player is either weapon restricted or played less than two playing hours.");
	    }
	    if(PlayerInfo[playerid][pFaction] >= 0 && PlayerInfo[targetid][pFaction] != PlayerInfo[playerid][pFaction])
	    {
	        return SCM(playerid, COLOR_ERROR, "[ERROR]:"WHITE" You can only give away weapons to your own faction members.");
	    }

	    GivePlayerWeaponEx(targetid, weaponid);
	    RemovePlayerWeapon(playerid, weaponid);

	    SCMf(targetid, COLOR_SERVER, "%s has given you their %s.", GetRPName(playerid), GetWeaponNameEx(weaponid));
	    SCMf(playerid, COLOR_SERVER, "You have given %s your %s.", GetRPName(targetid), GetWeaponNameEx(weaponid));

