	return 1;
}

CMD:givecarvoucher(playerid, params[])
{
    new targetid, amount;

    if(PlayerInfo[playerid][pAdmin] < 7)
    {
        return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" You are not authorized to use this command.");
    }
    if(sscanf(params, "ui", targetid, amount))
    {
        return SCM(playerid, COLOR_SYNTAX, "Usage: /givecarvoucher [playerid] [amount]");
    }
    if(!IsPlayerConnected(targetid))
    {
        return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" The specified player has been disconnected.");
    }
    if(!PlayerInfo[targetid][pLogged])
    {
        return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: That player is not logged in yet.");
    }
    PlayerInfo[targetid][pVoucher0] += amount;

    mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE users SET Voucher0 = %i WHERE uid = %i", PlayerInfo[targetid][pVoucher0], PlayerInfo[targetid][pID]);
    mysql_tquery(connectionID, queryBuffer);

    SAM(COLOR_YELLOW, "[SYSTEM]: "WHITE" %s has given %i Car Voucher to %s.", GetRPName(playerid), amount, GetRPName(targetid));
    Log_Write("log_admin", "%s (uid: %i) has used /givecarvoucher to give %i to %s (uid: %i).", GetPlayerNameEx(playerid), PlayerInfo[playerid][pID], amount, GetPlayerNameEx(targetid), PlayerInfo[targetid][pID]);

    return 1;
}

