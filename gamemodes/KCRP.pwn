DisplayStats(playerid, targetid = INVALID_PLAYER_ID)
{
	if(targetid == INVALID_PLAYER_ID) targetid = playerid;

	new super[268], str[3000], titlestring[250];
    new name[24], gender[8], faction[48], facrank[32], gang[32], gangrank[32], insurance[24], division[32], Float:health, Float:armor;

	if(playerid == MAX_PLAYERS)
	{
		strcpy(name, PlayerInfo[playerid][pUsername]);
	}
	else
	{
		strcat(name, GetRPName(playerid));
	}

	if(PlayerInfo[playerid][pGender] == 1) gender = "Male";
	else if(PlayerInfo[playerid][pGender] == 2) gender = "Female";
	else if(PlayerInfo[playerid][pGender] == 3) gender = "Shemale";

	switch(PlayerInfo[playerid][pInsurance])
	{
	    case HOSPITAL_COUNTY: insurance = "County General Hospital";
	    case HOSPITAL_ALLSAINTS: insurance = "Allsaints Hospital";
	    case HOSPITAL_SF1: insurance = "Kerala General Hospital";
	    default: insurance = "None";
	}

	if(PlayerInfo[playerid][pFaction] >= 0)
	{
	    if(!strcmp(FactionInfo[PlayerInfo[playerid][pFaction]][fShortName], "None", true))
	    {
		    strcpy(faction, FactionInfo[PlayerInfo[playerid][pFaction]][fName]);
		}
		else
		{
		    strcpy(faction, FactionInfo[PlayerInfo[playerid][pFaction]][fShortName]);
		}

	    format(facrank, sizeof(facrank), "%s (%i)", FactionRanks[PlayerInfo[playerid][pFaction]][PlayerInfo[playerid][pFactionRank]], PlayerInfo[playerid][pFactionRank]);

	    if(PlayerInfo[playerid][pDivision] >= 0)
	    {
	        strcpy(division, FactionDivisions[PlayerInfo[playerid][pFaction]][PlayerInfo[playerid][pDivision]]);
		}
		else
		{
		    division = "None";
		}
	}
	else
	{
	    faction = "None";
	    facrank = "N/A";
	    division = "None";
	}

	if(PlayerInfo[playerid][pGang] >= 0)
	{
	    strcpy(gang, GangInfo[PlayerInfo[playerid][pGang]][gName]);
	    strcpy(gangrank, GangRanks[PlayerInfo[playerid][pGang]][PlayerInfo[playerid][pGangRank]]);
	}
	else
	{
	    gang = "None";
	    gangrank = "N/A";
	}

	if(playerid == MAX_PLAYERS)
	{
	    health = PlayerInfo[playerid][pHealth];
	    armor = PlayerInfo[playerid][pArmor];
	}
	else
	{
		GetPlayerHealth(playerid, health);
		GetPlayerArmour(playerid, armor);
	}
	format(super, sizeof(super), "[%s] %s", name, GetDate());
	strcat(titlestring, super);
 	format(super, sizeof(super), "{FFFFFF}Level: %i - Hours: %i - Gender: %s - Age: %i - Cash: %s - Bank: %s - Phone: %i - DM Warns: %i/5\n", PlayerInfo[playerid][pLevel], PlayerInfo[playerid][pHours], gender, PlayerInfo[playerid][pAge], FormatNumber(PlayerInfo[playerid][pCash]), FormatNumber(PlayerInfo[playerid][pBank]), PlayerInfo[playerid][pPhone], PlayerInfo[playerid][pDMWarnings]);
 	strcat(str, super);
	format(super, sizeof(super), "{E08B3E}Wealth: %s - Crimes: %i - Arrests: %i - Channel: %i - Report Warnings: %i/3\n", FormatNumber(PlayerInfo[playerid][pCash] + PlayerInfo[playerid][pBank]), PlayerInfo[playerid][pCrimes], PlayerInfo[playerid][pArrested], PlayerInfo[playerid][pChannel], PlayerInfo[playerid][pReportWarns]);
 	strcat(str, super);
	format(super, sizeof(super), "{FFFFFF}Job: %s[%i] / %s[%i] - Weapon Restriction: %i Hrs - Insurance: %s - Married to: %s\n", GetJobName(PlayerInfo[playerid][pJob]), GetJobLevel(playerid, PlayerInfo[playerid][pJob]), GetJobName(PlayerInfo[playerid][pSecondJob]), GetJobLevel(playerid, PlayerInfo[playerid][pSecondJob]), PlayerInfo[playerid][pWeaponRestricted], insurance, PlayerInfo[playerid][pMarriedName]);
 	strcat(str, super);
    format(super, sizeof(super), "{E08B3E}Faction: %s [%s] - Division: %s - Gang: %s [%s] - Diamonds: %s - Warnings: %i\n", faction, facrank, division, gang, gangrank, FormatNumber(PlayerInfo[playerid][pDiamonds], 0), PlayerInfo[playerid][pWarnings]);
 	strcat(str, super);
	if(PlayerInfo[targetid][pAdmin] > 0)
	{
	    format(super, sizeof(super), "{FFFFFF}Interior: %i - Virtual: %i - Reports: %i - Help Requests: %i - AM Count: %i - Vip Package: %s", (playerid == MAX_PLAYERS) ? (PlayerInfo[playerid][pInterior]) : (GetPlayerInterior(playerid)), (playerid == MAX_PLAYERS) ? (PlayerInfo[playerid][pWorld]) : (GetPlayerVirtualWorld(playerid)), PlayerInfo[playerid][pReports], PlayerInfo[playerid][pHelpRequests], PlayerInfo[playerid][pAMCount], GetDonatorRank(PlayerInfo[playerid][pDonator]));
 		strcat(str, super);
	}
	ShowPlayerDialog(targetid, DIALOG_NONE, DIALOG_STYLE_MSGBOX, titlestring, str, "Okay", "");
	return 1;
}

