	    foreach(new i : Player)
	    {
	        if(PlayerInfo[i][pShowLands])
	        {
	            GangZoneShowForPlayer(i, LandInfo[landid][lGangZone], (LandInfo[landid][lOwnerID] > 0) ? (0x99ffbeAA) : (0x33CC33AA));
			}
		}
	}
}

