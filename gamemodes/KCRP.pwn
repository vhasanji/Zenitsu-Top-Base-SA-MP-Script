			SendProximityMessage(playerid, 20.0, COLOR_SERVER, "**{C2A2DA} Stranger(B%d) %.*s...", MaskaraID[playerid], MAX_SPLIT_LENGTH, params);
			SendProximityMessage(playerid, 20.0, COLOR_SERVER, "**{C2A2DA} ...%s", params[MAX_SPLIT_LENGTH]);
		}
		else
		{
			SendProximityMessage(playerid, 20.0, COLOR_SERVER, "**{C2A2DA} Stranger(B%d) %s", MaskaraID[playerid], params);
		}
	} else {
