	}

	Log_Write("log_vip", "%s (uid: %i) has given %s (uid: %i) a %s Donator VIP for %i days.", GetPlayerNameEx(playerid), PlayerInfo[playerid][pID], GetPlayerNameEx(targetid), PlayerInfo[targetid][pID], GetDonatorRank(rank), days);
