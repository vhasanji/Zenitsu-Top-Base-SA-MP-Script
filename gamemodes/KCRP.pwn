        if(BusinessInfo[i][bOutdoor])
        {
            if(BusinessInfo[i][bExists] && IsPlayerInRangeOfPoint(playerid, 8.0, BusinessInfo[i][bPosX], BusinessInfo[i][bPosY], BusinessInfo[i][bPosZ]))
