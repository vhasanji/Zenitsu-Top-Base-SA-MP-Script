	}
	if(!(1.0 <= radius <= 100.0))
	{
	    return SCM(playerid, COLOR_WHITE, "The radius can't be below 1.0 or above 100.0.");
	}

	foreach(new i : Player)
	{
		if(IsPlayerInRangeOfPlayer(i, playerid, radius))
