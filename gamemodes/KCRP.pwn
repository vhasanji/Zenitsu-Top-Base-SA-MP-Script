CMD:bantime(playerid, params[])
{
	new targetid, days, reason[128];

    if(!PlayerInfo[playerid][pBanAppealer])
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" You are not authorized to use this command.");
	}
	if(PlayerInfo[playerid][pAdmin] < 6)
	{
		return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" You are not authorized to use this command.");
	}
	if(sscanf(params, "uii", targetid, days, reason))
	{
	    return SCM(playerid, COLOR_SYNTAX, "[USAGE]: /ban [playerid] [reason]");
	}
	if(!IsPlayerConnected(targetid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" The specified player has been disconnected.");
	}
	if(PlayerInfo[targetid][pAdmin] > PlayerInfo[playerid][pAdmin])
	{
	    return SCM(playerid, COLOR_SYNTAX, "The player specified has a higher admin level than you. They cannot be banned.");
	}
	if(PlayerInfo[targetid][pAdmin] == 7)
	{
 		SAM(COLOR_YELLOW, "Warning: %s has been autokicked for trying to ban a Community Founder (%s).", GetRPName(playerid), GetRPName(targetid));
 		KickPlayer(playerid);
 		return 1;
	}
	Log_Write("log_punishments", "%s (uid: %i) banned %s (uid: %i), reason: %s", GetPlayerNameEx(playerid), PlayerInfo[playerid][pID], GetPlayerNameEx(targetid), PlayerInfo[targetid][pID], reason);
	
    PlayerInfo[targetid][pBANTime] = gettime() + (days * 86400);
    BanPlayer(targetid, GetPlayerNameEx(playerid), reason);
    if(days >= 30)
	{
		SMA(COLOR_YELLOW, "[SYSTEM]: "WHITE"  %s was banned by %s for %i month(s), reason: %s", GetRPName(targetid), GetRPName(playerid), days / 30, reason);
		new admwrn[128];
	    format(admwrn,sizeof(admwrn),"```AdmCmd: %s was banned by %s for %i month(s), reason: %s  ```", GetRPName(targetid), GetRPName(playerid), days / 30, reason);
	    //DCC_SendChannelMessage(admin, admwrn);
	}
	else
	{
		SMA(COLOR_YELLOW, "[SYSTEM]: "WHITE"  %s was banned by %s for %i month(s), reason: %s", GetRPName(targetid), GetRPName(playerid), days, reason);
		new admwrn[128];
	    format(admwrn,sizeof(admwrn),"```AdmCmd: %s was banned by %s for %i month(s), reason: %s  ```", GetRPName(targetid), GetRPName(playerid), days, reason);
	    //DCC_SendChannelMessage(admin, admwrn);
	}
    return 1;
}

