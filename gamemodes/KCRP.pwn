CMD:lahi(playerid, params[])
{
	new type;

	if(sscanf(params, "i", type))
	{
	    SCM(playerid, COLOR_SYNTAX, "[USAGE]: "LIME" /lahi [type]");
	    SCM(playerid, COLOR_GREY2, "List of accents: [0] Filipino - [1] Tagalog - [2] Bisaya - [3] Mangyan - [4] Ilonggo - ");
	    SCM(playerid, COLOR_GREY2, "List of accents: [5] Kapampangan - [6] Batangenyo - [7] Bikolano - [7] Muslim");
		SCM(playerid, COLOR_SERVER, "For more accent contact the developers.");
		return 1;
	}

	switch(type)
	{
		case 0: strcpy(PlayerInfo[playerid][pAccent], "Filipino", 16);
		case 1: strcpy(PlayerInfo[playerid][pAccent], "Tagalog", 16);
		case 2: strcpy(PlayerInfo[playerid][pAccent], "Bisaya", 16);
		case 3: strcpy(PlayerInfo[playerid][pAccent], "Mangyan", 16);
		case 4: strcpy(PlayerInfo[playerid][pAccent], "Ilonggo", 16);
		case 5: strcpy(PlayerInfo[playerid][pAccent], "Kapampangan", 16);
		case 6: strcpy(PlayerInfo[playerid][pAccent], "Batangenyo", 16);
		case 7: strcpy(PlayerInfo[playerid][pAccent], "Bikolano", 16);
		case 8: strcpy(PlayerInfo[playerid][pAccent], "Muslim", 16);

		default: SCM(playerid, COLOR_SYNTAX, "INVALID ACCENT. VALID TYPES RANGE FROM 0 TO 3.");
	}

	SM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" YOU SET YOUR ACCENT TO '%s'.", PlayerInfo[playerid][pAccent]);

	mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE users SET accent = '%e' WHERE uid = %i", PlayerInfo[playerid][pAccent], PlayerInfo[playerid][pID]);
	mysql_tquery(connectionID, queryBuffer);

	return 1;
}

