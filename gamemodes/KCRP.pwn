ReloadLand(landid)
{
	if(LandInfo[landid][lExists])
	{
	    new
	        string[128];

		DestroyDynamic3DTextLabel(LandInfo[landid][lText]);
		DestroyDynamicPickup(LandInfo[landid][lPickup]);
	    DestroyDynamicArea(LandInfo[landid][lArea]);
