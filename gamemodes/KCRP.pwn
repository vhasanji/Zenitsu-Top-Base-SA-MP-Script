public OnIncomingConnection(playerid, ip_address[], port)
{
    new
        count = 1;
 
    for (new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
    {
        if (IsPlayerConnected(i) && strcmp(GetPlayerIP(i), ip_address, false) == 0) 
        {
            count ++;
        }
    }
 
    if (count > MAX_CONNECTIONS)
    {
        Kick(playerid);
    }
    return true;
}
 
stock GetPlayerIP(playerid)
{
    new
        str[39 + 1];
 
    GetPlayerIp(playerid, str, sizeof(str));
    return str;
}
public OnDynamicObjectMoved(objectid)
{
    new Float:x, Float:y, Float:z;
	for(new i; i < sizeof(Obj_FloorDoors); i ++)
	{
		if(objectid == Obj_FloorDoors[i][0])
		{
		    GetDynamicObjectPos(Obj_FloorDoors[i][0], x, y, z);

		    if(x < X_DOOR_L_OPENED - 0.5)   // Some floor doors have shut, move the elevator to next floor in queue:
		    {
				Elevator_MoveToFloor(ElevatorQueue[0]);
				RemoveFirstQueueFloor();
			}
		}
	}

	if(objectid == Obj_Elevator)   // The elevator reached the specified floor.
	{
	    KillTimer(ElevatorBoostTimer);  // Kills the timer, in case the elevator reached the floor before boost.

	    FloorRequestedBy[ElevatorFloor] = INVALID_PLAYER_ID;

	    Elevator_OpenDoors();
	    Floor_OpenDoors(ElevatorFloor);

	    GetDynamicObjectPos(Obj_Elevator, x, y, z);
	    Label_Elevator	= Create3DTextLabel("{CCCCCC}Press '{FFFFFF}~k~~CONVERSATION_YES~{CCCCCC}' to use elevator", 0xCCCCCCAA, 1784.9822, -1302.0426, z - 0.9, 4.0, 0, 1);

	    ElevatorState 	= ELEVATOR_STATE_WAITING;
	    SetTimer("Elevator_TurnToIdle", ELEVATOR_WAIT_TIME, 0);
	}

	return 1;
}
public OnPlayerDisconnect(playerid, reason)
{

	new PlayerName[25];
	GetPlayerName(playerid, PlayerName, sizeof(PlayerName));
	printf("%s Just left the server.", PlayerName);
	
	SavePlayerVariables(playerid);
    // REMOVING THE PLAYER'S LOCAL STREAM AFTER DISCONNECTING
    if (lstream[playerid])
    {
        SvDeleteStream(lstream[playerid]);
        lstream[playerid] = SV_NULL;
    }
    if(IsValidActor(PlayerInfo[playerid][pMedicActor]))
		DestroyActor(PlayerInfo[playerid][pMedicActor]);
		
	new string[128];
	format(string, sizeof(string), " ```%s has left the server```", GetRPName(playerid));
    

	TerminateInfo(playerid, reason);
	return 1;
}
public OnPlayerRequestClass(playerid, classid)
{
    if(PlayerInfo[playerid][pKicked]) return 0;
    if(IsPlayerNPC(playerid)) return 1;

	if(!PlayerInfo[playerid][pLogged])
	{
	    ClearChat(playerid);
    	for(new i = 0; i < 5; i ++)
		{
		    SCM(playerid, -1, " ");
		}

		TogglePlayerSpectating(playerid, 1);
		SetPlayerColor(playerid, 0xFFFFFF00);

  		SetTimerEx("ShowMainMenuCamera", 400, false, "i", playerid);
	}
	return 1;
}
public OnPlayerSpawn(playerid)
{
        //=====[ AUDIO LIVE BRODCASTING SYSTEM ]=====//
	PlayAudioStreamForPlayer(playerid, "http://stream.zeno.fm/1pqqs4rd0chvv");
    TogglePlayerControllable(playerid, 0);
    
    
    TextDrawShowForPlayer(playerid, RanDomNewsTD[1]);
    TextDrawShowForPlayer(playerid, RanDomNewsTD[0]);
    SetTimerEx("UpdateRNW", 200000, true, "i", playerid);

    PlayerInfo[playerid][pGPSOn] = 1;

    StopAudioStreamForPlayer(playerid);
    SetPlayerStamina(playerid, GetPlayerMaxStamina(playerid));
    if(TalpakData[e_bTalpak] && TalpakData[e_iRedPlayerid] == playerid || TalpakData[e_bTalpak] && TalpakData[e_iBluePlayerid] == playerid)
    {
        PlayerInfo[playerid][pInjured] = 0;
        EndTalpakEvent();
        return 1;
    }

    if(PlayerInfo[playerid][pKicked]) return 0;
	PreloadAnims(playerid);

	if(PlayerInfo[playerid][pPaintball])
	{
	    SetPlayerInPaintball(playerid);
		UnfreezePlayerEx(playerid);
	}
	else if(PlayerInfo[playerid][pPaintball2])
	{
	    SetPlayerInPaintball2(playerid);
		UnfreezePlayerEx(playerid);
	}
    else if(PlayerInfo[playerid][pJailType] == 3)
