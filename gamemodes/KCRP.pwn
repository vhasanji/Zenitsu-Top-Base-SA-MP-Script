CMD:bring(playerid, params[]) return callcmd::gethere(playerid, params);
CMD:gethere(playerid, params[])
{
	new targetid;

    if(PlayerInfo[playerid][pAdmin] < 1)
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" You are not authorized to use this command.");
	}
	if(!PlayerInfo[playerid][pAdminDuty] && PlayerInfo[playerid][pAdmin] < 5)
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" This command requires you to be on admin duty. /aduty to go on duty.");
	}
	if(sscanf(params, "u", targetid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "[USAGE]: "LIME" /bring [PLAYERID]");
	}
	if(!IsPlayerConnected(targetid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" The specified player has been disconnected.");
	}
	if(PlayerInfo[targetid][pAdmin] > PlayerInfo[playerid][pAdmin])
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" The player specified has a higher admin level than you. They cannot be teleported.");
	}
    if(PlayerInfo[targetid][pPaintball])
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" That player is currently in the paintball arena.");
	}

	TeleportToPlayer(targetid, playerid);
	SM(playerid, COLOR_GREY2, "Teleported %s to your position.", GetRPName(targetid));

	return 1;
}

