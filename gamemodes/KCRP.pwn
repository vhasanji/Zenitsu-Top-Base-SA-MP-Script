CMD:detain(playerid, params[])
{
	new targetid, vehicleid = GetPlayerVehicleID(playerid);
    if(!IsLawEnforcement(playerid) && GetFactionType(playerid) != FACTION_DOC && GetFactionType(playerid) != FACTION_GOVERNMENT)
