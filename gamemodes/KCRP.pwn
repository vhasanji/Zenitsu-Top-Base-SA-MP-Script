	else
	{
		SAM(COLOR_YELLOW, "[SYSTEM]: "WHITE" %s has given a {C2A2DA}%s{FF6347} VIP package to %s for %i day(s).", GetRPName(playerid), GetDonatorRank(rank), GetRPName(targetid), days);
