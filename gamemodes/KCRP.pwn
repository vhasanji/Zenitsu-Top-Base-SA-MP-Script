	{
	    format(str, sizeof(str), "%sThis player has a (%d) of Raw Ephedrine.\n", str, PlayerInfo[targetid][pEphedrine]);
	}

	switch(PlayerInfo[targetid][pSmuggleDrugs])
	{
	    case 1: format(str, sizeof(str), "%s{FF0000}Seeds package"WHITE"\n\n", str);
	    case 2: format(str, sizeof(str), "%s{FF0000}Crack package"WHITE"\n\n", str);
