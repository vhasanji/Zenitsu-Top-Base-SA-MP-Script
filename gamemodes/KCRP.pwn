		if(VehicleInfo[vehicleid][vCrash])
		{
			switch(random(5))
			{
			    case 0:
			    {
		    		SetVehicleParams(vehicleid, VEHICLE_ENGINE, true);
		    		SendProximityMessage(playerid, 20.0, COLOR_SERVER, "**{C2A2DA} %s turns on the engine of the %s.", GetRPName(playerid), GetVehicleName(vehicleid));
		    		VehicleInfo[vehicleid][vCrash] = 0;
		    		return 1;
				}
				case 1:
				{
				    SetVehicleParams(vehicleid, VEHICLE_ENGINE, false);
					SendProximityMessage(playerid, 20.0, COLOR_SERVER, "**{C2A2DA} %s attempts to turn on the engine of the %s but fails.", GetRPName(playerid), GetVehicleName(vehicleid));
					return 1;
				}
 			    case 2:
			    {
		    		SetVehicleParams(vehicleid, VEHICLE_ENGINE, true);
		    		SendProximityMessage(playerid, 20.0, COLOR_SERVER, "**{C2A2DA} %s turns on the engine of the %s.", GetRPName(playerid), GetVehicleName(vehicleid));
		    		VehicleInfo[vehicleid][vCrash] = 0;
		    		return 1;
				}
				case 3:
				{
				    SetVehicleParams(vehicleid, VEHICLE_ENGINE, false);
					SendProximityMessage(playerid, 20.0, COLOR_SERVER, "**{C2A2DA} %s attempts to turn on the engine of the %s but fails.", GetRPName(playerid), GetVehicleName(vehicleid));
					return 1;
				}
				case 4:
				{
  					SetVehicleParams(vehicleid, VEHICLE_ENGINE, true);
		    		SendProximityMessage(playerid, 20.0, COLOR_SERVER, "**{C2A2DA} %s turns on the engine of the %s.", GetRPName(playerid), GetVehicleName(vehicleid));
