CMD:makeyoutuber(playerid, params[])
{
	new targetid, level;

    if(PlayerInfo[playerid][pAdmin] < 7)
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" You are not authorized to use this command.");
	}
	if(sscanf(params, "u", targetid))
	{
	    return SCM(playerid, COLOR_LIGHTBLUE, "[USAGE]:{ffffff} /makeyoutuber [playerid]");
	}
	if(!IsPlayerConnected(targetid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" The specified player has been disconnected.");
	}
	if(!PlayerInfo[targetid][pLogged])
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: That player is not logged in yet.");
	}

	SendStaffMessage(COLOR_YELLOW, "[SYSTEM]: "WHITE" %s has given a youtube role to %s.", GetRPName(playerid), GetRPName(targetid));
	PlayerInfo[targetid][pYoutuber] = 1;

	mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE users SET youtuber = %i WHERE uid = %i", level, PlayerInfo[targetid][pID]);
	mysql_tquery(connectionID, queryBuffer);

	SM(playerid, COLOR_WHITE, "You have give a "RED"youtuber"WHITE" role to %s.", GetRPName(targetid), GetRPName(targetid));
	SM(targetid, COLOR_WHITE, "%s has gave you a "RED"youtuber"WHITE" role.", GetRPName(playerid));

	Log_Write("log_admin", "%s (uid: %i) set %s's (uid: %i) youtuber role", GetPlayerNameEx(playerid), PlayerInfo[playerid][pID], GetPlayerNameEx(targetid), PlayerInfo[targetid][pID]);
	return 1;
}

CMD:removeyoutuber(playerid, params[])
{
	new targetid, level;

    if(PlayerInfo[playerid][pAdmin] < 7)
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" You are not authorized to use this command.");
	}
	if(sscanf(params, "u", targetid))
	{
	    return SCM(playerid, COLOR_LIGHTBLUE, "[USAGE]:{ffffff} /makeyoutuber [playerid]");
	}
	if(!IsPlayerConnected(targetid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" The specified player has been disconnected.");
	}
	if(!PlayerInfo[targetid][pYoutuber])
	{
		SCM(playerid, COLOR_SYNTAX,"This player is not a youtuber, perhaps youjizzer.");
	}
	if(!PlayerInfo[targetid][pLogged])
	{
	    return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: That player is not logged in yet.");
	}

	SendStaffMessage(COLOR_YELLOW, "[SYSTEM]: "WHITE"  %s has removed a youtube role to %s.", GetRPName(playerid), GetRPName(targetid));
