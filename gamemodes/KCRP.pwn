CMD:placehvault(playerid, params[])
{
	new houseid = GetInsideHouse(playerid);

	if(houseid == -1)
		return SM(playerid, COLOR_GREY, "You are not inside the house that you own.");

	if(!IsHouseOwner(playerid, houseid))
		return SM(playerid, COLOR_TWEET, "You either don't own a house, or are not at one of them.");

	if(isnull(params))
		return SM(playerid, COLOR_WHITE, "USAGE: /placehvault [vault password]");

	if(strlen(params) < 4 || strlen(params) > 32)
		return SM(playerid, COLOR_GREY, "Invalid vault password length.");

	strcpy(HouseInfo[houseid][hSafePass], params, 32);

	new string[255];
	format(string, sizeof(string), "* Your house vault password is set to '%s'", HouseInfo[houseid][hSafePass]);
	SM(playerid, COLOR_WHITE, string);

	GetPlayerPos(playerid, HouseInfo[houseid][hVaultX], HouseInfo[houseid][hVaultY], HouseInfo[houseid][hVaultZ]);
	HouseInfo[houseid][gStashInterior] = GetPlayerInterior(playerid);
	HouseInfo[houseid][gStashWorld] = GetPlayerVirtualWorld(playerid);

	mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE `houses` SET stash_x = '%f', stash_y = '%f', stash_z = '%f', stashinterior = %i, stashworld = %i WHERE id = %i", HouseInfo[houseid][hVaultX], HouseInfo[houseid][hVaultY], HouseInfo[houseid][hVaultZ], HouseInfo[houseid][gStashInterior], HouseInfo[houseid][gStashWorld], HouseInfo[houseid][hID]);
	mysql_tquery(connectionID, queryBuffer);
	mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE `houses` SET house_safe = '%e' WHERE id = %i", HouseInfo[houseid][hSafePass], HouseInfo[houseid][hID]);
	mysql_tquery(connectionID, queryBuffer);
	ReloadHouse(houseid);

	SM(playerid, COLOR_YELLOW, "You have placed your house safe in your position.");

	return 1;
}

CMD:hvault(playerid, params[])
{
	new houseid = GetInsideHouse(playerid);

	if(houseid == -1)
		return SM(playerid, COLOR_GREY, "You are not inside the house that you own.");

	if(!IsValidDynamicPickup(HouseInfo[houseid][vaPickup]))
		return SM(playerid, COLOR_GREY, "You do not have a house vault placed in yet.");

	if(HouseInfo[houseid][hSafeOpen])
	{
		HouseInfo[houseid][hSafeOpen] = 0;
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s has shut closed their house vault.", GetPlayerNameEx(playerid));


		mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE `houses` SET house_safe = %d WHERE id = %i", HouseInfo[houseid][hSafeOpen], HouseInfo[houseid][hID]);
		mysql_tquery(connectionID, queryBuffer);
		ReloadHouse(houseid);

	}
	else
	{
		if(isnull(params))
			return SM(playerid, COLOR_GREY, "USAGE: /hvault [vault password]");

		if(!strcmp(HouseInfo[houseid][hSafePass], params, true))
		{
			HouseInfo[houseid][hSafeOpen] = 1;
			SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "*  %s has opened their house vault.", GetPlayerNameEx(playerid));

			mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE `houses` SET house_safe = %d WHERE id = %i", HouseInfo[houseid][hSafeOpen], HouseInfo[houseid][hID]);
			mysql_tquery(connectionID, queryBuffer);
			ReloadHouse(houseid);

			//format(string, sizeof(string), "%s opened house vault of house ID %d.", GetPlayerNameEx(playerid), houseid);
			//Log("house", string);
		}
		else
		{
			SM(playerid, COLOR_TWEET, "You have inputted the wrong safe password.");
			SM(playerid, COLOR_YELLOW, "HINT: /changemyvaultpass to change your vault password.");
		}
	}
	return 1;
}

CMD:changemyvaultpass(playerid, params[])
{
	new houseid = GetInsideHouse(playerid), string[128];

	if(houseid == -1)
		return SM(playerid, COLOR_GREY, "You are not inside the house that you own.");

	if(!IsHouseOwner(playerid, houseid))
		return SM(playerid, COLOR_TWEET, "You either don't own a house, or are not at one of them.");

	if(!IsValidDynamicPickup(HouseInfo[houseid][vaPickup]))
		return SM(playerid, COLOR_GREY, "You do not have a house vault placed in yet.");

	if(isnull(params))
		return SM(playerid, COLOR_WHITE, "USAGE: /changemyvaultpass [vault password]");

	if(strlen(params) < 4 || strlen(params) > 32)
		return SM(playerid, COLOR_GREY, "Invalid vault password length.");

	strcpy(HouseInfo[houseid][hSafePass], params, 32);
	mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE `houses` SET house_safe = '%e' WHERE id = %i", HouseInfo[houseid][hSafePass], HouseInfo[houseid][hID]);
	mysql_tquery(connectionID, queryBuffer);
	ReloadHouse(houseid);

	format(string, sizeof(string), "You have changed your house vault password to: %s.", params);
	SM(playerid, COLOR_YELLOW, string);
	return 1;
}

CMD:stash(playerid, params[])
{
	new houseid;

 	if((houseid = GetInsideHouse(playerid)) >= 0)
	{
	    new option[14], param[32];
	    if(!(IsPlayerInRangeOfPoint(playerid, 5.0, HouseInfo[houseid][hVaultX], HouseInfo[houseid][hVaultY], HouseInfo[houseid][hVaultZ]) && GetPlayerVirtualWorld(playerid) == HouseInfo[houseid][gStashWorld]))
	    {
			return SendClientMessage(playerid, COLOR_GREY, "You are not near your house vault.");
		}
		if(!HouseInfo[houseid][hLevel])
		{
		    return SCM(playerid, -1, ""GREEN"[NOTICE] "WHITE"This house has no stash upgrade. '/upgradehouse level' to purchase one.");
	    }
		if(!HouseInfo[houseid][hSafeOpen])
			return SM(playerid, COLOR_GREY, ""GREEN"[NOTICE] "WHITE"The safe is not open. Open it using /hvault.");
	    if(PlayerInfo[playerid][pAdminDuty])
	    {
			return SendClientMessage(playerid, COLOR_GREY, "You cannot use this command while on admin duty");
		}
		if(sscanf(params, "s[14]S()[32]", option, param))
		{
		    return SCM(playerid, -1, ""BLUE"[USAGE] "WHITE" /stash [balance | deposit | withdraw]");
	    }
	    if(!strcmp(option, "balance", true))
	    {
	        new count;

	        for(new i = 0; i < 10; i ++)
	        {
	            if(HouseInfo[houseid][hWeapons][i])
	            {
	                count++;
	            }
	        }

	        SCM(playerid, COLOR_GREY, "Stash Balance:");
	        SM(playerid, COLOR_GREY2, "Cash: $%i/$%i", HouseInfo[houseid][hCash], GetHouseStashCapacity(houseid, STASH_CAPACITY_CASH));
			SM(playerid, COLOR_GREY2, "Materials: %i/%i | Weapons: %i/%i", HouseInfo[houseid][hMaterials], GetHouseStashCapacity(houseid, STASH_CAPACITY_MATERIALS), count, GetHouseStashCapacity(houseid, STASH_CAPACITY_WEAPONS));
	        SM(playerid, COLOR_GREY2, "Pot: %i/%i grams | Crack: %i/%i grams", HouseInfo[houseid][hPot], GetHouseStashCapacity(houseid, STASH_CAPACITY_WEED), HouseInfo[houseid][hCrack], GetHouseStashCapacity(houseid, STASH_CAPACITY_COCAINE));
	        SM(playerid, COLOR_GREY2, "Meth: %i/%i grams | Painkillers: %i/%i pills", HouseInfo[houseid][hMeth], GetHouseStashCapacity(houseid, STASH_CAPACITY_METH), HouseInfo[houseid][hPainkillers], GetHouseStashCapacity(houseid, STASH_CAPACITY_PAINKILLERS));
            SCM(playerid, COLOR_GREY, "Stash Ammunition:");
			SM(playerid, COLOR_GREY2, "HP Ammo: %i/%i | Poison Ammo: %i/%i", HouseInfo[houseid][hHPAmmo], GetHouseStashCapacity(houseid, STASH_CAPACITY_HPAMMO), HouseInfo[houseid][hPoisonAmmo], GetHouseStashCapacity(houseid, STASH_CAPACITY_POISONAMMO));
            SM(playerid, COLOR_GREY2, "FMJ Ammo: %i/%i", HouseInfo[houseid][hFMJAmmo], GetHouseStashCapacity(houseid, STASH_CAPACITY_FMJAMMO));

			if(count > 0)
			{
				SCM(playerid, COLOR_GREY, "Stash Weapons:");

            	for(new i = 0; i < 10; i ++)
	            {
    	            if(HouseInfo[houseid][hWeapons][i])
	    	        {
	        	        SM(playerid, COLOR_GREY2, "[%i] Weapon: %s", i + 1, GetWeaponNameEx(HouseInfo[houseid][hWeapons][i]));
					}
				}
	        }
		}
		else if(!strcmp(option, "deposit", true))
	    {
	        new value;

	        if(sscanf(param, "s[14]S()[32]", option, param))
	        {
	            SCM(playerid, COLOR_SYNTAX, "Usage: /stash [deposit] [option]");
	            SCM(playerid, COLOR_WHITE, "Available options: Cash, Materials, Pot, Crack, Meth, Painkillers, Weapon");
	            SCM(playerid, COLOR_WHITE, "Available options: HPAmmo, PoisonAmmo, FMJAmmo");
	            return 1;
	        }
	        if(!strcmp(option, "cash", true))
			{
			    if(sscanf(param, "i", value))
			    {
			        return SCM(playerid, COLOR_SYNTAX, "Usage: /stash [deposit] [cash] [amount]");
				}
				if(value < 1 || value > PlayerInfo[playerid][pCash])
				{
				    return SCM(playerid, COLOR_SYNTAX, "Insufficient amount.");
			    }
			    if(GetHouseStashCapacity(houseid, STASH_CAPACITY_CASH) < HouseInfo[houseid][hCash] + value)
			    {
			        return SM(playerid, COLOR_SYNTAX, "Your stash can only hold up to $%i at your house's level.", GetHouseStashCapacity(houseid, STASH_CAPACITY_CASH));
			    }

			    GivePlayerCash(playerid, -value);
			    HouseInfo[houseid][hCash] += value;

			    mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE houses SET cash = %i WHERE id = %i", HouseInfo[houseid][hCash], HouseInfo[houseid][hID]);
			    mysql_tquery(connectionID, queryBuffer);

