	if(strfind(newval, "|") != -1)
	{
	    return SCM(playerid, COLOR_SYNTAX, "You may not include the '|' character in the MOTD.");
	}
	if(!strcmp(option, "global", true))
	{
	    if(PlayerInfo[playerid][pAdmin] < 7) return SCM(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" You are not authorized to use this command.");
 		if(!strcmp(newval, "none", true))
