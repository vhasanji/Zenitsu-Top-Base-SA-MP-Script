CMD:sendlocation(playerid, params[])
{
	new targetid;
	if(!PlayerInfo[playerid][pPhone])
	{
	    return SendClientMessage(playerid, COLOR_SYNTAX, "You don't have a cellphone to see location your friend");
	}
 	if(sscanf(params, "u", targetid))
	{
	    return SendClientMessage(playerid, COLOR_SYNTAX, "Usage: /sendlocto [playerid]");
	}
	if(!IsPlayerConnected(targetid))
	{
	    return SendClientMessage(playerid, COLOR_SYNTAX, "[SYSTEM]: "RED" The specified player has been disconnected.");
	}
	if(targetid == playerid)
	{
	    return SendClientMessage(playerid, COLOR_SYNTAX, "You can't use this command on yourself.");
	}
	if(GetPlayerInterior(playerid))
	{
	    return SendClientMessage(playerid, COLOR_SYNTAX, "You are inside interior. You can't send your location at them at the moment.");
	}
	if(PlayerInfo[targetid][pAdminDuty])
	{
	    return SendClientMessage(playerid, COLOR_SYNTAX, "You can't use this command on an on-duty administrator.");
	}
	PlayerInfo[targetid][pFindTime] = 20;
    PlayerInfo[targetid][pDetectiveCooldown] = 28;
    SetPlayerMarkerForPlayer(targetid, playerid, 0xFF0000FF);
	SendMessage(targetid, COLOR_WHITE, " %s's location marked on your radar.", GetRPName(playerid));
	SendMessage(playerid, COLOR_WHITE, " You have sent your location to %s.", GetRPName(targetid));

	PlayerInfo[targetid][pFindPlayer] = playerid;
	return 1;
}

