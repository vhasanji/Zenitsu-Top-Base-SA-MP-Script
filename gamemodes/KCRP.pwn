					}
					if(value < 1 || value > PlayerInfo[playerid][bpHPAmmo])
					{
					    return SCM(playerid, COLOR_SYNTAX, "Insufficient amount.");
				    }
				    if(PlayerInfo[playerid][pHPAmmo] + value > GetPlayerCapacity(CAPACITY_HPAMMO))
				    {
				        return SM(playerid, COLOR_SYNTAX, "You currently have %i/%i HP ammo. You can't carry anymore until you upgrade your inventory skill.", PlayerInfo[playerid][pHPAmmo], GetPlayerCapacity(CAPACITY_HPAMMO));
